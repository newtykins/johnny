{
    "[rust]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "rust-lang.rust-analyzer"
    },
    "files.exclude": {
        "**/target": false,
        "Cargo.lock": false,
        "src/build_data.rs": false,
        "assets": false,
        "code_of_conduct.md": false,
        "license.md": false,
        ".pre-commit-config.yaml": false,
        "template": false,
        "packages/pride": false,
        "docker": false,
        ".cargo": false,
        "packages/autorole": false,
        "packages/bot/src/build_data.rs": false
    },
    "rust-analyzer.cargo.features": "all",
    "workspace.isHidden": false
}